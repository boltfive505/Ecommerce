<Page x:Class="Egate_Ecommerce.Pages.setup_page"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Egate_Ecommerce.Pages"
      xmlns:pages="clr-namespace:Egate_Ecommerce.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="setup_page">
    <Grid Background="White">
        <TabControl HorizontalContentAlignment="Left" TabStripPlacement="Left" Padding="0" Margin="0" Background="White">
            <TabItem Header="Employee List" Padding="{StaticResource TabItemPadding}" Background="{StaticResource Create.Button.Background}">
                <Frame Margin="{StaticResource FrameMargin}">
                    <Frame.Content>
                        <pages:employee_list_page/>
                    </Frame.Content>
                </Frame>
            </TabItem>
        </TabControl>
    </Grid>
</Page>
